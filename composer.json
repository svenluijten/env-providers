{
    "name": "sven/env-providers",
    "description": "Load Laravel service providers based on your application's environment.",
    "keywords": [
        "provider",
        "laravel",
        "environment",
        "serviceprovider"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "contact@svenluijten.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.5.1"
    },
    "autoload": {
        "psr-4": {
            "Sven\\EnvProviders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\EnvProviders\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\EnvProviders\\ServiceProvider"
            ]
        }
    }
}
